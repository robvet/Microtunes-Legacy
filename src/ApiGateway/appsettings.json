{
  "HttpClientRetryCount": "3",
  "HttpClientExceptionsAllowedBeforeBreaking": "5",
  "HttpClientTimeout": 240, // seconds - set high so you can debug without timeout
  "HttpClientEnableResiliency": true, //Toggles resiliency off/on (true or false()
  "TelemetryNameForService": "Gateway",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}